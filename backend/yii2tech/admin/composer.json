{
    "name": "yii2tech/admin",
    "description": "Admin pack (actions, widgets, etc) for Yii2",
    "keywords": ["yii2", "admin"],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yii2tech/admin/issues",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "https://github.com/yii2tech/admin/wiki",
        "source": "https://github.com/yii2tech/admin"
    },
    "authors": [
        {
            "name": "Paul Klimov",
            "email": "klimov.paul@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "*"
    },
    "require-dev": {
        "yiisoft/yii2-bootstrap": ">=2.0.5"
    },
    "suggest": {
        "yiisoft/yii2-bootstrap": "you need this package, if you wish to use provided widgets"
    },
    "autoload": {
        "psr-4": { "yii2tech\\admin\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
